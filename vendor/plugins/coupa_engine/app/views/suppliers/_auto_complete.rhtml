<li>
  <span class="acid" style="display:none"><%= auto_complete.id %></span>
  <span class="acstatus" style="display:none"><%= auto_complete.status %></span>
  <span class="acname"><b><%= auto_complete.name %></b><%= auto_complete.status == 'draft' ? '<i>(Draft)</i>' : '' %></span><br/>
  <span style="color:#999">
    <span class="acaddress" style="display:none"><%= h address_to_html(auto_complete.primary_address) %></span>
    <span class="acemail"><%= auto_complete.primary_contact.email %></span>
  </span>
</li>
